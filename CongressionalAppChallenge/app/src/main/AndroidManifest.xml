<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.risingtide.congressionalappchallenge">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/eagle"
        android:label="Voter Registration"
        android:roundIcon="@mipmap/eagle_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".HomeScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".candidatesActivity"></activity>
        <activity android:name=".registerToVote"></activity>
    </application>

</manifest>