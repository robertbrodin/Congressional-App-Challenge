<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.risingtide.congressionalappchallenge">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/votelogo_round"
        android:label="Register to Vote!"
        android:roundIcon="@mipmap/votelogo_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".HomeScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".candidatesActivity"></activity>
        <activity android:name=".registerToVote"></activity>
        <activity android:name=".quizActivity"></activity>
        <activity android:name=".bakerActivity"></activity>
        <activity android:name=".individualCandidates"></activity>
        <activity android:name=".missionActivity"></activity>
        <activity android:name=".ballotQuestions"></activity>
    </application>

</manifest>
